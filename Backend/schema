	`keyname`			varchar, 
	`keyvalue`			varchar
);

CREATE TABLE `t_attachment`
 (
	`attachmentid`			INTEGER, 
	`externalfilepath`			TEXT, 
	`filename`			varchar, 
	`localfileflag`			varchar, 
	`md5hash`			varchar, 
