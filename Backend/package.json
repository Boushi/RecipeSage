{
  "name": "chefbook-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "mocha": "PORT=2999 NODE_ENV=test mocha --timeout 10000 --exit --",
    "test": "npm run test:clean && npm run test:all",
    "test:ci": "npm run test:all",
    "test:clean": "rm test.sqlite3 || true",
    "test:all": "npm run test:routes && npm run test:services && npm run test:models",
    "test:routes": "npm run mocha \"./routes/**/*.spec.js\"",
    "test:services": "npm run mocha \"./services/**/*.spec.js\"",
    "test:models": "npm run mocha \"./models/**/*.spec.js\""
  },
  "dependencies": {
    "aws-sdk": "^2.200.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~4.1.1",
    "elasticsearch": "^15.4.1",
    "express": "~4.16.4",
    "express-grip": "^1.1.0",
    "extract-zip": "^1.6.7",
    "firebase-admin": "^5.9.1",
    "fs-extra": "^7.0.1",
    "gm": "^1.23.1",
    "grip": "^1.2.8",
    "mdb": "git+https://git@github.com/julianpoy/node-mdb.git",
    "moment": "^2.20.1",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "multer-imager": "git+https://git@github.com/julianpoy/multer-imager.git",
    "multer-s3": "^2.7.0",
    "node-cron": "^2.0.3",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.3",
    "puppeteer": "^1.12.2",
    "raven": "^2.6.2",
    "request": "^2.83.0",
    "sequelize": "^4.43.0",
    "xml-js": "^1.6.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "sequelize-cli": "^5.4.0",
    "sinon": "^7.2.2",
    "sqlite3": "^4.0.6",
    "supertest": "^3.3.0"
  }
}
