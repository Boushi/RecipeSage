{
  "name": "prisma",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/prisma/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/prisma",
        "command": "prisma generate --schema=./src/prisma/schema.prisma && tsc"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/prisma",
        "command": "tsc --noEmit"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/prisma",
        "command": "eslint ."
      }
    }
  }
}
