{
  "name": "RecipeSage",
  "version": "0.0.0",
  "author": "Julian Poyourow",
  "homepage": "https://recipesage.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "dist": "shx rm -rf www/* && npm run build --prod && npm run purifycss && node ./cache-busting.js && workbox injectManifest && npm run compress",
    "open": "http-server www -o -a 0.0.0.0 -p 1234 -g -c-1",
    "compress": "bread-compressor -a gzip www",
    "purifycss": "purifycss www/build/main.css www/build/*.js --info --min --out www/build/main.css --whitelist .bar-button-default"
  },
  "config": {
    "ionic_sass": "./sass.config.js",
    "ionic_copy": "./copy.config.js",
    "ionic_webpack": "./webpack.config.js"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic/pro": "2.0.4",
    "@ionic/storage": "2.2.0",
    "@ngx-loading-bar/core": "^2.1.2",
    "@sentry/browser": "^4.6.4",
    "@types/firebase": "^3.2.1",
    "blueimp-load-image": "^2.20.1",
    "cordova-browser": "5.0.4",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^2.3.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "dayjs": "^1.7.8",
    "firebase": "^5.7.1",
    "fraction.js": "^4.0.11",
    "ionic-angular": "3.9.2",
    "ionicons": "4.5.1",
    "linkifyjs": "^2.1.7",
    "ngx-virtual-scroller": "^1.0.16",
    "rxjs": "^5.5.11",
    "workbox-sw": "^3.6.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.11",
    "bread-compressor-cli": "^1.0.4",
    "http-server": "^0.11.1",
    "purify-css": "^1.2.6",
    "rev-hash": "^2.0.0",
    "shx": "^0.3.2",
    "typescript": "~2.4.2",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-cli": "^3.6.3",
    "worker-loader": "^2.0.0"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-fcm": {}
    },
    "platforms": [
      "browser"
    ]
  }
}
