{
    "rules": {
        "keysOnViews": "error",
        "zombieKeys": "warning",
        "misprintKeys": "warning",
        "maxWarning": "0",
        "misprintCoefficient": "0.9",
        "deepSearch": "enable",
        "ignoredKeys": [
          "====== (.*)"
        ]
    },
    "project": "./src/app/**/*.{html,ts}",
    "languages": "./src/assets/i18n/*.json"
}
